{
  "name": "swoft-fork/log",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "log",
    "swoft-log",
    "monolog"
  ],
  "description": "swoft log component",
  "homepage": "https://github.com/sky-up/swoft-log",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.0",
    "monolog/monolog": "^1.24",
    "toolkit/cli-utils": "^1.0",
    "swoft-fork/stdlib": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Log\\": "src/"
    }
  },
  "require-dev": {
    "swoft-fork/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Log\\Unit\\": "test/unit/",
      "SwoftTest\\Log\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
